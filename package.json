{
  "name": "node-red-contrib-xbee",
  "version": "1.0.0",
  "description": "Node-RED module for the Xbee radio modules",
  "main": "index.js",
  "scripts": {
    "prestart": "bash ./init.sh",
    "start": "node node_modules/node-red/red.js --userDir .node-red",
    "test": "node node_modules/jshint/bin/jshint ./lib/xbee.js ./lib/xbeepool.js ./lib/xbee.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ramonmata/node-red-contrib-xbee.git"
  },
  "keywords": [
    "node-red",
    "xbee"
  ],
  "node-red": {
    "nodes": {
      "xbee": "lib/xbee-red.js"
    }
  },
  "author": "Ram√≥n Mata <ramon@mata.com.mx> (http://ramon.mata.com.mx)",
  "homepage": "https://github.com/ramonmata/node-red-contrib-xbee.git#readme",
  "bugs": {
    "url": "https://github.com/ramonmata/node-red-contrib-xbee/issues"
  },
  "license": "MIT",
  "dependencies": {
    "serialport": "^4.0.7",
    "xbee-api": "^0.5.2"
  },
  "devDependencies": {
    "jshint": "^2.9.4",
    "node-red": "^0.16.2"
  }
}
